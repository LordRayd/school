	CLO
	MOV AL,[64]
	MOV BL,[65]
	CMP AL,BL
	JNS SI_ANOTINFB
SI_AINFB:
	MOV BL,64
	JMP FINSI
SI_ANOTINFB:
	MOV BL,65
FINSI:
	MOV AL,[BL]
	INC AL
	MOV [BL],AL
	ORG 64
	DB 1
	DB 2
	END

	CLO
	MOV BL,64
BOUCLE:
	IN 00
	CMP AL,0D
	JZ FIN
	MOV [BL],AL
	INC BL
	JMP BOUCLE
FIN:
	MOV AL,0
	MOV [BL],AL
	END

	MOV CL,C0
	MOV BL,64
	MOV AL,[BL]
ENDROIT:
	CMP AL,0
	JZ MOITIE
	MOV [CL],AL
	INC BL
	INC CL
	JMP ENDROIT
MOITIE:
	DEC BL
	CMP BL,64
	JS FIN
	MOV AL,[BL]
	MOV [CL],AL
	INC CL
	JMP MOITIE
FIN:
	END